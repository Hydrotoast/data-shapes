# baseURI: http://www.w3.org/ns/shacl/test/features/core/maxInclusive-001

@prefix ex: <http://example.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<>
	a sh:Graph ;
	sh:shapesGraph <http://www.w3.org/ns/shacl> ;
.


# Shapes ----------------------------------------------------------------------

ex:MyShape
	a sh:Shape ;
	sh:scopeNode ex:ValidResource1 ;
	sh:scopeNode ex:ValidResource2 ;
	sh:scopeNode ex:InvalidResource1 ;
	sh:scopeNode ex:InvalidResource2 ;
	sh:property [
		sh:predicate ex:property ;
		sh:maxInclusive 1 ;
	] ;
.


# Instances -------------------------------------------------------------------

# Valid
ex:ValidResource1
	ex:property 0 ;
.

# Valid
ex:ValidResource2
	ex:property 1 ;
.

# Invalid: > value
ex:InvalidResource1
	ex:property 2 ;
.

# Invalid: cannot compare
ex:InvalidResource2
	ex:property "a" ;
.
