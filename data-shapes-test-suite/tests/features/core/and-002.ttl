# baseURI: http://www.w3.org/ns/shacl/test/features/core/and-002

@prefix ex: <http://example.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<>
	a sh:Graph ;
	sh:shapesGraph <http://www.w3.org/ns/shacl> ;
.


# Shape Definitions -----------------------------------------------------------

ex:SuperShape
	a sh:Shape ;
	sh:property [
		sh:predicate ex:property ;
		sh:minCount 1 ;
	] ;
.

ex:AndShape
	a sh:Shape ;
	sh:scopeNode ex:ValidInstance1 ;
	sh:scopeNode ex:InvalidInstance1 ;
	sh:scopeNode ex:InvalidInstance2 ;
	sh:constraint [
		a sh:AndConstraint ;
		sh:shapes (
			ex:SuperShape
			[
				sh:property [
					sh:predicate ex:property ;
					sh:maxCount 1 ;
				]
			]
		)
	] ;
.

# Instances -------------------------------------------------------------------

ex:ValidInstance1
	ex:property "One" ;
.

# Invalid: missing property
# ex:InvalidInstance1

# Invalid: more than one property
ex:InvalidInstance2
	ex:property "One" ;
	ex:property "Two" ;
.
