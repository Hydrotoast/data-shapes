	a sh:Graph ;
	sh:shapesGraph <http://www.w3.org/ns/shacl> ;
.

# 2: cardinality, no value constraints, open

ex:myShape
	a sh:Shape ;
	sh:property [
		sh:predicate dct:creator ;
		sh:maxCount 2 ;
	] ;
.



# Instances -------------------------------------------------------------------

ex:instance1
	sh:nodeShape ex:MyShape ;
  	dct:creator "A";
.

ex:instance2
	sh:nodeShape ex:MyShape ;
  	dct:creator "A";
	dct:creator "B" ;
	dct:title "Moby" ;
.

ex:instance3
    sh:nodeShape ex:myShape ;
