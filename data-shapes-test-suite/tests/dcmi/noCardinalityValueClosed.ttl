
	
# 8: no cardinality, value constraint, closed	

@prefix ex: <http://example.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dct:  <http://purl.org/dc/terms#> .
@prefix skos:     <http://www.w3.org/2004/02/skos/core#> .
@prefix foaf:     <http://xmlns.com/foaf/0.1/#> .

<>
	a sh:Graph ;
	sh:shapesGraph <http://www.w3.org/ns/shacl> ;
.


ex:myShape
	a sh:Shape ;
	sh:property [
		sh:predicate dct:creator ;
		sh:nodeKind sh:IRI ;
	] ;
	sh:constraint [
		a sh:ClosedShapeConstraint ;
		sh:ignoredProperties ( sh:nodeShape rdf:type ) ;
	] ;
.

# Instances -------------------------------------------------------------------

ex:instance1 #ok
	sh:nodeShape ex:MyShape ;
  	dct:creator <http://id.loc.gov/ex> ;
.

ex:instance2 #fail
	sh:nodeShape ex:MyShape ;
  	dct:creator <http://id.loc.gov/ex> ;
	dct:creator "A" ;
.

ex:instance3 #fail
    sh:nodeShape ex:myShape ;
	dct:creator "A" ;
.

ex:instance4 #ok
	sh:nodeShape ex:myShape ;
	dct:creator <http://id.loc.gov/ex> ;
	dct:creator <http://id.loc.gov/ex2> ;
.

ex:instance5 #ok
	sh:nodeShape ex:myShape ;
	dct:creator <http://id.loc.gov/ex> ;
	dct:creator <http://id.loc.gov/ex2> ;
	dct:creator <http://id.loc.gov/ex3> ;
.

ex:instance6 #fail
	sh:nodeShape ex:myShape ;
	dct:creator <http://id.loc.gov/ex> ;
	dct:creator <http://id.loc.gov/ex2> ;
	dct:title "Moby" ;
.
