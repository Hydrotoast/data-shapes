@prefix mf: <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix sht: <http://www.w3.org/ns/shacl-test#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix ex: <http://example.org/shacl-test/> .

<>
  rdf:type mf:Manifest ;
  mf:entries (
      <bind>
    ) ;
.

<bind> rdf:type sht:Validate;
  rdfs:label "Test of pre-binding with a BIND construct using an unbound $this" ;
  mf:action [ sht:dataGraph <bind-data.ttl> ;
              sht:shapesGraph <bind-shapes.ttl> ] ;
  mf:result [ rdf:type sh:ValidationReport ;
              sh:conforms "false"^^xsd:boolean ;
	      sh:result [ rdf:type sh:ValidationResult ;
 	      		  sh:resultSeverity sh:Violation ;
 			  sh:focusNode ex:i ;
 			  sh:value ex:j ;
			  sh:sourceConstraint ex:c ;
 			  sh:sourceConstraintComponent sh:SPARQLConstraintComponent ] ] ;
  mf:status sht:proposed .
