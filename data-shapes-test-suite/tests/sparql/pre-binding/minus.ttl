@prefix mf: <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix sht: <http://www.w3.org/ns/shacl-test#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix ex: <http://example.org/shacl-test/> .

<> rdf:type mf:Manifest ;
  mf:entries (
      <minus>
    ) .

<minus> rdf:type sht:Validate;
  rdfs:label "Test of pre-binding flipping a MINUS to the shared binding state" ;
  mf:action [ sht:dataGraph <minus-data.ttl> ;
              sht:shapesGraph <minus-shapes.ttl> ] ;
  mf:result [ rdf:type sh:ValidationReport ;
              sh:conforms "true"^^xsd:boolean ] ;
  mf:status sht:proposed .
