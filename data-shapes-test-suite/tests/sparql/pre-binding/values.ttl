@prefix mf: <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix sht: <http://www.w3.org/ns/shacl-test#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix ex: <http://example.org/shacl-test/> .

<>
  rdf:type mf:Manifest ;
  mf:entries (
      <value>
    ) ;
.

<value> rdf:type sht:Validate;
  rdfs:label "Test of pre-binding with only a VALUE construct" ;
  mf:action [ sht:dataGraph <value-data.ttl> ;
              sht:shapesGraph <value-shapes.ttl> ] ;
  mf:result [ rdf:type sh:ValidationReport ;
              sh:conforms "false"^^xsd:boolean ;
	      sh:result [ rdf:type sh:ValidationResult ;
 	      		  sh:resultSeverity sh:Violation ;
 			  sh:focusNode 5 ;
 			  sh:value 5 ;
			  sh:sourceConstraint "SELECT $this WHERE { VALUES $this { 5 } }" ;
 			  sh:sourceConstraintComponent sh:SPARQLConstraintComponent ] ] ;
  mf:status sht:proposed .
