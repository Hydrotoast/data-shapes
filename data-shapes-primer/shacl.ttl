# SHACL schema for SHACL

PREFIX sh:<http://www.w3.org/ns/shacl/core#>
PREFIX xsd:<http://www.w3.org/2001/XMLSchema#>

sh:ShapeShape a sh:Shape ;
    sh:property [
        sh:property a ;
        sh:allowedValue sh:Shape ;
        sh:min 0; sh:max 1 ;
    ] ;
    sh:property [
        sh:property sh:property ;
        sh:valueShape sh:PropertyShape ;
        sh:min 0 ;
    ] ;
    sh:property [
        sh:property sh:inverseProperty ;
        sh:valueShape sh:InversePropertyShape ;
        sh:min 0 ;
    ] ;
    sh:property [
        sh:property sh:choice ;
        sh:valueShape sh:ShapeShape ;
        sh:min 0 ;
    ] ;
    sh:property [
        sh:property sh:propertyGroup ;
        sh:valueShape sh:ShapeShape ;
        sh:min 0 ;
    ] ;
 .
sh:PropertyShape a sh:Shape ;
    sh:property [
        sh:property sh:predicate ;
        sh:nodeType sh:IRI ;
        sh:min 1; sh:max 1 ;
    ] ;
    se:choice [
        sh:property [
            sh:property sh:allowedValue ;
            # some sh:nodeType for permitting anything?
            sh:min 1 ;
        ] ;
        sh:property [
            sh:property sh:valueShape ;
            sh:nodeType sh:IRI ;
            sh:min 1; sh:max 1 ;
        ] ;
        sh:property [
            sh:property sh:valueType ;
            sh:nodeType sh:IRI ;
            sh:min 1; sh:max 1 ;
        ] ;
        sh:property [
            sh:property sh:nodeType ;
            sh:nodeType sh:IRI ;
            sh:min 1; sh:max 1 ;
        ] ;    ] ;
    sh:property [
        sh:property sh:minCount ;
        sh:valueType xsd:integer ;
        sh:min 0; sh:max 1 ;
    ] ;
    sh:property [
        sh:property sh:maxCount ;
        sh:valueType xsd:integer ;
        sh:min 0; sh:max 1 ;
    ] ;
 .
sh:InversePropertyShape a sh:Shape ;
    sh:property [
        sh:property sh:predicate ;
        sh:nodeType sh:IRI ;
        sh:min 1; sh:max 1 ;
    ] ;
    se:choice [
        sh:property [
            sh:property sh:allowedValue ;
            # some sh:nodeType for permitting anything?
            sh:min 1 ;
        ] ;
        sh:property [
            sh:property sh:valueShape ;
            sh:nodeType sh:IRI ;
            sh:min 1; sh:max 1 ;
        ] ;
        sh:property [
            sh:property sh:nodeType ;
            sh:nodeType sh:IRI ;
            sh:min 1; sh:max 1 ;
        ] ;
    ] ;
    sh:property [
        sh:property sh:minCount ;
        sh:valueType xsd:integer ;
        sh:min 0; sh:max 1 ;
    ] ;
    sh:property [
        sh:property sh:maxCount ;
        sh:valueType xsd:integer ;
        sh:min 0; sh:max 1 ;
    ] ;
 .



